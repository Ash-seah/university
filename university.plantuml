@startuml

class User {
    id: int
    username: str
    email: str
    hashed_password: str
    is_active: bool
}

class Classes {
    id: int
    name: str
    description: str
    owner_id: int
}

class Riazi {
    id: int
    name: str
    description: str
    class_id: int
}

class Tajrobi {
    id: int
    name: str
    description: str
    class_id: int
}

class Ensani {
    id: int
    name: str
    description: str
    class_id: int
}

class Teachers {
    id: int
    name: str
    subject: str
    class_id: int
}

User --> Classes : owns
Classes --> Riazi : contains
Classes --> Tajrobi : contains
Classes --> Ensani : contains
Classes --> Teachers : contains

@enduml